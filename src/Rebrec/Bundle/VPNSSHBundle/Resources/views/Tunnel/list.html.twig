
{% extends "::base.html.twig" %}

{% block title %}Tunnels:list{% endblock %}

{% block body %}
{% spaceless %}
<h1>Tunnels Allou√©s</h1>
    <br>
    <table class="table table-striped table-hover">
        <thead>
            <tr>
                <th>Nom</th>
                <th>IP</th>
                <th>Port</th>
                <th>Protocol</th>
            </tr>
        </thead>
        <tbody>
            {% for tunnel in arrTunnels %}
                <tr>
                    <td>{{ tunnel.name }}</td>
                    <td>{{ tunnel.hostip }}</td>
                    <td>{{ tunnel.hostport }}</td>
                    <td>{{ tunnel.protocolname }}</td>
                </tr>
            {% endfor %}
        </tbody>
{%endspaceless %}
{% endblock %}
